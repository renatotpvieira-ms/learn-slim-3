{
    "name": "aurmil/slim2-skeleton",
    "description": "A Slim Framework 2 application skeleton",
    "keywords": [
        "slimphp",
        "slim",
        "framework"
    ],
    "authors": [
        {
            "name": "AurÃ©lien Millet",
            "homepage": "http://www.aurelien-millet.fr/",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/aurmil/slim2-skeleton",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "3.12.3",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "App\\": "App",
            "App\\Controller\\": "App/Controllers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "5.6.9"
        },
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}